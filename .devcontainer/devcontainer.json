{
	"name": "learning",
	"dockerComposeFile": "docker-compose.yml",
	"service": "development-machine",
	"workspaceFolder": "/workspace/learning",
	"forwardPorts": [
		3000,
		8000,
		8534,
		8533,
		8080
	],
	"postCreateCommand": ". ${containerWorkspaceFolder}/.devcontainer/setup-deps",
	"remoteEnv": {
		"GIT_AUTHOR_NAME": "${localEnv:GIT_AUTHOR_NAME}",
		"GIT_AUTHOR_EMAIL": "${localEnv:GIT_AUTHOR_EMAIL}",
		"GIT_COMMITTER_NAME": "${localEnv:GIT_COMMITTER_NAME}",
		"GIT_COMMITTER_EMAIL": "${localEnv:GIT_COMMITTER_EMAIL}",
		"VISUAL": "code --wait",
	},
	"extensions": [
		"teabyii.ayu",
		"ecmel.vscode-html-css",
		"streetsidesoftware.code-spell-checker",
		"usernamehw.errorlens",
		"asvetliakov.vscode-neovim",
		"file-icons.file-icons",
		"editorconfig.editorconfig",
		"mutantdino.resourcemonitor",
		"wakatime.vscode-wakatime",
		"mhutchie.git-graph",
		"eamodio.gitlens",
		"xabikos.JavaScriptSnippets",
		"yzhang.markdown-all-in-one",
		"robole.marky-stats",
		"sdras.night-owl",
		"esbenp.prettier-vscode",
		"stkb.rewrap",
		"DavidAnson.vscode-markdownlint",
		"tombonnike.vscode-status-bar-format-toggle",
		"bradlc.vscode-tailwindcss",
		"akamud.vscode-theme-onedark",
		"sdras.vue-vscode-extensionpack",
		"heybourn.headwind",
		"skellock.just",
		"dbaeumer.vscode-eslint"
	]
}
