    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.0",
    "frontend-commons": "github:IgnisDa/frontend-commons",
    "prettier": "~2.2.1",
    "prettier-plugin-svelte": "^2.4.0",
    "sass": "^1.38.2",
    "svelte": "^3.42.4",
    "svelte-check": "^2.2.5",
    "svelte-preprocess": "^4.8.0",
